.suspendButton {
  position: fixed;
  right: 10rpx;
  bottom: 24rpx;
  z-index: 2;
}
.suspendButton .su_Button {
  width: 64rpx;
  height: 64rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0;
  border-radius: 50%;
  background: #c4c4c4;
  border: none;
  margin-bottom: 10rpx;
}
.suspendButton .su_Button .img {
  display: block;
  width: 34rpx;
  height: 34rpx;
}
.suspendButton .su_Button::after {
  border: none;
}
.suspendButton .pop .pop_main .pop_main_center {
  height: 488rpx;
  width: 460rpx;
}
.suspendButton .pop .pop_main .pop_main_center .img {
  width: 100%;
  width: 100%;
  display: block;
}
.suspendButton .pop .pop_main .pop_main_footer {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 200rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  background: #fff;
}
.suspendButton .pop .pop_main .pop_main_footer .pop_Button {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  padding: 0;
  background: #fff;
}
.suspendButton .pop .pop_main .pop_main_footer .pop_Button::after {
  border: none;
}
.suspendButton .pop .pop_main .pop_main_footer .pop_Button .img {
  width: 76rpx;
  height: 76rpx;
  display: block;
}
.suspendButton .pop .pop_main .pop_main_footer .pop_Button text {
  color: #242424;
  font-size: 25rpx;
}

