.home .status_bar {
  height: calc(25px + 80rpx);
  width: 100%;
  position: fixed;
  top: 0;
  background: #fff;
  z-index: 10;
}
.home .top_view {
  height: 25px;
  width: 100%;
  color: #000000;
}
.home .geduan {
  height: calc(25px + 80rpx);
}
.home .titles {
  height: 80rpx;
}
.home .titles .titleText {
  height: 80rpx;
  color: #000000;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 34rpx;
  margin-left: 42rpx;
  font-weight: 600;
}
.home .banner {
  margin-top: 50rpx;
}
.home .banner .swiper {
  height: 260rpx;
}
.home .banner .swiper swiper-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.home .banner .swiper .swiper-item {
  width: 690rpx;
  height: 260rpx;
}
.home .banner .swiper .swiper-item ._img {
  width: 100%;
  height: 100%;
  display: block;
}
.home .banner .page-section-spacing {
  position: relative;
}
.home .banner .page-section-spacing .dot {
  position: absolute;
  bottom: 20rpx;
}
.home .home_main {
  padding-bottom: 32rpx;
}
.home .home_main .popular_product {
  margin-top: 69rpx;
  padding: 0 30rpx;
}
.home .home_main .popular_product .product_title {
  font-size: 30rpx;
  font-weight: 600;
  margin-left: 8rpx;
}
.home .home_main .popular_product .product_center {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  border-radius: 7rpx;
  border: 1rpx solid #e9edf3;
  margin-top: 23rpx;
}
.home .home_main .popular_product .product_center .product_center_item {
  height: 127rpx;
  width: 338rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.home .home_main .popular_product .product_center .product_center_item .img {
  width: 47rpx;
  margin-left: 20rpx;
  margin-right: 18rpx;
  display: block;
  height: 100%;
}
.home .home_main .popular_product .product_center .product_center_item .text {
  font-size: 26rpx;
}
.home .home_main .popular_product .product_center .border_btm {
  border-bottom: 1rpx solid #e9edf3;
}
.home .home_main .popular_product .product_center .border_rgt {
  border-right: 1rpx solid #e9edf3;
}
.home .PhoneNumber {
  opacity: 0;
  height: 100vh;
  width: 100vw;
  position: fixed;
  top: 0;
  left: 0;
}

